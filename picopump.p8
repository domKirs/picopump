__lua__

local Physics = {}
Physics.world = {}

function Account:newRectangle(x, y, w, h, gravity)
    local o = {}
    o.x, o.y = x, y
    o.w, o.h = w, h
    o.gravity = gravity
    o.airTimer = 0
    o.isGrounded = true

    setmetatable(o, self)
    self.__index = self
    return o
end





